<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.login.AdminProfileController">

    <Label layoutX="150" layoutY="20" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Admin Profile" />

    <Label fx:id="username_ID" layoutX="50" layoutY="60" text="Username:" />
    <TextField fx:id="profileUsername" editable="false" layoutX="150" layoutY="60" prefWidth="200" />

    <Label fx:id="Password" layoutX="52.0" layoutY="115.0" text="Password:" />

    <Button fx:id="profile" layoutX="147.0" layoutY="177.0" onAction="#loadProfile" prefWidth="105.0" text="Load Profile" />
   <Button fx:id="btn_back" layoutX="179.0" layoutY="223.0" mnemonicParsing="false" onAction="#loadPrevious" text="BACK" />
   <TextField fx:id="profilePassword" layoutX="150.0" layoutY="111.0" prefHeight="25.0" prefWidth="200.0" />

</AnchorPane>
